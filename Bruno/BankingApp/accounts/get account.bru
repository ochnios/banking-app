meta {
  name: get account
  type: http
  seq: 1
}

get {
  url: {{baseUrl}}/api/user/account
  body: none
  auth: none
}

script:pre-request {
  const cookie = bru.getVar("cookie");
  
  if(cookie) {
    req.setHeader("Cookie", cookie);
  }
}

script:post-response {
  const cookies = res.getHeader('set-cookie');
  
  if(cookies) {
    bru.setVar("cookie", cookies.join('; '));
  }
}
